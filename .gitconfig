[color]
  ui = true
[alias]
  tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[core]
  excludesfile = ~/.gitignore
  editor = /usr/local/bin/vim
[ghq]
  root = ~/go/src
[push]
  default = simple
